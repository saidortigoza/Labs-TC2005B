    <title>Series favoritas</title>
    <link rel = "stylesheet" href = "../css/styles.css">
</head>
<body>
    <h1 class = "title">Series favoritas de Said</h1>
    <p>Estas son las series favoritas de Said:</p>
    <ol>
        <% for (let serie in series) { %>
        <li><%= series[serie] %></li>
    <%} %>
    </ol>
    <div>
        <a href = "/series/new">
            <button class = "boton">Agregar nueva serie</button>
        </a>

        <br>
        
        <a href = "../">
            <button class = "boton">Regresar a Inicio</button>
        </a>
        
        <br>

        <a href = "pending">
            <button class = "boton">Series favoritas (Not Found)</button>
        </a>
    </div>
    
    <%- include('includes/footer.ejs') %>
</body>
</html>